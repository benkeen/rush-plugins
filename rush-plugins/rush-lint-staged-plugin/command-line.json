{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/command-line.schema.json",
  "commands": [
    {
      "name": "lint-staged",
      "commandKind": "global",
      "summary": "Rush plugin for lint-staged",
      "shellCommand": "node <packageFolder>/lib/index.js",
      "safeForSimultaneousRushProcesses": true
    }
  ],
  "parameters": [
    {
      "argumentName": "MAX_ARG_LENGTH",
      "associatedCommands": ["lint-staged"],
      "description": "maximum length of the command-line argument string",
      "longName": "--max-arg-length",
      "parameterKind": "string",
      "required": false
    }
  ]
}
